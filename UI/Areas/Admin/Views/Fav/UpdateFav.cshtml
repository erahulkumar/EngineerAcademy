
@{
    ViewBag.Title = "UpdateFav";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<section class="content-header">
    <h2>Update Fav Logo Title</h2>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Admin</a></li>
        <li class="breadcrumb-item"><a>Fav</a></li>
        <li class="breadcrumb-item"><a>Update Fav Logo Tilte</a></li>
    </ol>
</section>
<br />
<section class="content">
    <div class="row">
        <div class="col-md-9">
            @Html.Partial("~/Areas/Admin/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("UpdateFav", "Fav", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/FavForm")
                }
            </div>
        </div>
    </div>
</section>


